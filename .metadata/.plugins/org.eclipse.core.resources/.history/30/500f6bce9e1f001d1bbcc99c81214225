package edu.java.loop05;

public class LoopMain05 {

    public static void main(String[] args) {
        // 369 게임 출력
        
        for(int i=1;i<101;i++) {
        
            String str = "*";
            
            if ((i % 10 == 3) || (i % 10 == 6) || (i % 10 == 9)) {
                System.out.print(str);
            }
             else if((i>29) || (i<40) || (i>59) && (i<70) || (i>89) || (i<100) ) {
                 System.out.print(str);
            }
            else{
                System.out.print(i + "\t");
                
                if(i % 10 == 0)
                System.out.print("\n");
           }
       }
        
    }
}
