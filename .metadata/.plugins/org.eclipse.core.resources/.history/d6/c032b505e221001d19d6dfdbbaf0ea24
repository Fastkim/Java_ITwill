package edu.java.loops08;

public class LoopMain08 {

    public static void main(String[] args) {
        // 중첩 반복문
        for(int dan = 2 ;dan < 10; dan++ ) {
            for(int i = 1; i < 10; i++) {
            System.out.println(dan + " x " + i + " = " + (i * dan));
            }
            System.out.println();
        }
           
    }

}
