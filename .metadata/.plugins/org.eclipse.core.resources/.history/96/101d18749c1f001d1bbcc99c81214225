package edu.java.loop05;

public class LoopMain05 {

    public static void main(String[] args) {
        // 369 게임 출력
        
        for(int i=1;i<101;i++) {
            System.out.print(i + "\t" + "김");
            
            if ((i % 10 == 3)&&(i % 10 == 6)&&(i % 10 == 9)) {
                System.out.print("*");
            }
            else if((i>29) && (i<40) && (i>59) && (i<70) && (i>89) && (i<100) ) {
                System.out.print("*");
            }
            else if (i % 10 == 0) {
                System.out.print("\n");
            }
       }
        
    }
}
