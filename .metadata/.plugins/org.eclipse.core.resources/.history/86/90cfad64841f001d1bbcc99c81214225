package edu.java.loop03;

public class LoopMain03 {

    public static void main(String[] args) {
        /* 아래와 같은 형식으로 구구단 2단을 출력하세요.
           2 x 1 = 2
           2 x 2 = 4
           2 x 3 = 6
           ...
           2 x 8 = 16
           2 x 9 = 18
         */

        for(int i=1;i<10;i++) {
            System.out.println("2 x " + i);
        }
    }

}
